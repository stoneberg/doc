select * from jojo_academy;
select * from jojo_subject;
	
	
select a.* from jojo_academy a inner join jojo_subject b
on b.academy_id = a."id";

select distinct a.* from jojo_academy a inner join jojo_subject b
on b.academy_id = a."id";
	
    select
        distinct academy0_.id as id1_0_0_,
        academy0_.name as name2_0_0_
    from
        jojo_academy academy0_ 
    left outer join
        jojo_subject subjects1_ 
            on academy0_.id=subjects1_.academy_id;	
	
	
	
